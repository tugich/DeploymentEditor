Global LogReaderWindow


Procedure OpenLogReaderWindow(x = 0, y = 0, width = 1000, height = 800)
  LogReaderWindow = OpenWindow(#PB_Any, x, y, width, height, "Software Log Reader", #PB_Window_SystemMenu | #PB_Window_MinimizeGadget | #PB_Window_MaximizeGadget | #PB_Window_SizeGadget | #PB_Window_ScreenCentered)
EndProcedure

Procedure LogReaderWindow_Events(event)
  Select event
    Case #PB_Event_CloseWindow
      ProcedureReturn #False

    Case #PB_Event_Menu
      Select EventMenu()
      EndSelect

    Case #PB_Event_Gadget
      Select EventGadget()
      EndSelect
  EndSelect
  ProcedureReturn #True
EndProcedure

