Procedure NewProjectWindow_Events(event)
  Select event
    Case #PB_Event_CloseWindow
      ProcedureReturn #False

    Case #PB_Event_Menu
      Select EventMenu()
      EndSelect

    Case #PB_Event_Gadget
      Select EventGadget()
        Case NPW_Recent1_Link_ProjectFile
          OpenFirstRecentProject(EventType())          
        Case NPW_Recent1_Link_ProjectPath
          OpenFirstRecentProject(EventType())          
        Case NPW_Recent1_Button_OpenFolder
          OpenFirstRecentFolder(EventType())          
        Case NPW_ImportMSI_Hyperlink
          NotAvailableFeatureMessage(EventType())          
        Case NPW_ImportEXE_Hyperlink
          NotAvailableFeatureMessage(EventType())          
        Case NPW_ImportWinGet_Hyperlink
          NotAvailableFeatureMessage(EventType())          
        Case NPW_Button_OpenOtherProject
          OpenOtherProject(EventType())          
        Case NPW_Button_NewEmptyProject
          CreateNewProject(EventType())          
        Case NPW_ButtonImage_Help
          ShowAboutWindow(EventType())          
        Case Button_Donation
          OpenDonationUrl(EventType())          
      EndSelect
  EndSelect
  ProcedureReturn #True
EndProcedure



